output "Samsung Electric Company SAMSUNG Unknown" {
    mode 1920x1080@60
    position 0,0
    scale 1.0
    alias $TV
}

output "Thermotrex Corporation TL156MDMP31-0 Unknown" {
    mode 3200x2000@120
    position 0,1080
    scale 1.67
    adaptive_sync on
    alias $INTERNAL
}

profile tv {
    output $TV enable
    output $INTERNAL disable
}

profile portable {
    output $INTERNAL enable position 0,0
}
